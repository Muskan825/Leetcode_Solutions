ïƒ°	class Solution {
  public int fib(int n) {
        int one=0;
        int two=1;
        if(n==1)
            return 1;
        if(n==2)
            return 1;
        int temp=0;
        for(int i=2;i<=n;i++){
            temp=one+two;
            one=two;
            two=temp;
        }
        return temp;
    }
}
